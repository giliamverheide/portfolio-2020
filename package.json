    "eslint-plugin-nuxt": "0.5.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-vue": "6.1.2",
    "image-webpack-loader": "6.0.0",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-pngquant": "8.0.0",
    "node-sass": "4.13.1",
    "prettier": "1.19.1",
    "sass-loader": "8.0.2",
